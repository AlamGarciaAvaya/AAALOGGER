                    },
                    {
                        "name": "LogMessage",
                        "displayPropName": "Log",
                        "type": "String",
                        "controlType": "multilineText",
                        "textLines": "7",
                        "modelName": "setLogMessage",
                        "mapping":"LogMessage"
                    }
                ],
	        "input_schema": "{
			        title: InputSchema,
			        type: object,
			        properties: {
			            LogMessage: {
			                type: string
			            },
			            Nivel: {
			                type: string
			            }

			        }
	        }",
                "output_schema": "{
                        title: OutputSchema,
                        type: object,
                        properties: {
                            status: {
                                type: string
                            }
